<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        {% block stylesheets %}
            <link rel="stylesheet" href="css/font-awesome-4.7.0/css/font-awesome.min.css"></script>
            <link rel="stylesheet" href="js/node_modules/bootstrap/dist/css/bootstrap.min.css"></script>
        {% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    </head>
    <body>
        {% block body %}
        {% endblock %}
        {% block javascripts %}
            <script type="text/javascript" src="js/node_modules/jquery/dist/jquery.min.js"></script>
            <script type="text/javascript" src="js/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
            <script type="text/javascript" src="js/build/bundle.js"></script>
        {% endblock %}
        
    </body>
</html>
